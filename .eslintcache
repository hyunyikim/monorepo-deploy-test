e24InterworkService.changeLeaveReason(mallId, reasons);\n\t\treturn;\n\t}\n\n\t@Get(':mallId/categories')\n\tgetCategories(\n\t\t@Param('mallId') mallId: string,\n\t\t@Query() query: Partial<CategoryListParams>\n\t) {\n\t\tconst {limit = 100, depth, offset = 0, name} = query;\n\t\treturn this.cafe24InterworkService.getCategories(mallId, {\n\t\t\tlimit,\n\t\t\tdepth,\n\t\t\toffset,\n\t\t\tname,\n\t\t});\n\t}\n}\n","/Users/hwanieee/Desktop/vircle/packages/cafe24-interwork/src/cafe24Interwork/cafe24Interwork.dto.ts",[],[],"/Users/hwanieee/Desktop/vircle/packages/cafe24-interwork/src/cafe24Interwork/cafe24Interwork.mock.ts",[],[],"/Users/hwanieee/Desktop/vircle/packages/cafe24-interwork/src/cafe24Interwork/cafe24Interwork.service.ts",[],[],"/Users/hwanieee/Desktop/vircle/packages/cafe24-interwork/src/cafe24Interwork/cafe24intework.master.controller.ts",[],[],"/Users/hwanieee/Desktop/vircle/packages/cafe24-interwork/src/cafe24Interwork/guaranteeReq.entity.ts",[],[],"/Users/hwanieee/Desktop/vircle/packages/cafe24-interwork/src/cafe24Interwork/index.ts",[],[],"/Users/hwanieee/Desktop/vircle/packages/cafe24-interwork/src/cafe24Interwork/interwork.entity.ts",["2612"],[],"import {Exclude, Type} from 'class-transformer';\nimport {\n\tIsArray,\n\tIsBoolean,\n\tIsDateString,\n\tIsNumber,\n\tIsObject,\n\tIsOptional,\n\tIsString,\n\tValidateNested,\n} from 'class-validator';\nimport {AccessToken, Category, Store} from '../cafe24Api';\n\nexport type IssueTiming = 'AFTER_SHIPPING' | 'AFTER_DELIVERED';\n\nexport interface IssueCategory {\n\tidx: number;\n\tdepth: number;\n\tname: string;\n\tfullName: string[];\n\tfullNo: number[];\n}\nexport class IssueSetting {\n\t@IsBoolean()\n\tmanually: boolean;\n\n\t@IsString()\n\tissueTiming: IssueTiming;\n\n\t@IsBoolean()\n\tissueAll: boolean;\n\n\t@IsArray()\n\tissueCategories: IssueCategory[];\n\n\t@IsBoolean()\n\tissueIntro?: boolean = true;\n}\n\nexport class Cafe24Interwork {\n\t@IsNumber()\n\tpartnerIdx: number;\n\n\t@IsString()\n\tmallI