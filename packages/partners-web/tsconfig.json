{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "@emotion/react",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"plugins": [
			{
				"name": "typescript-styled-plugin",
				"tags": ["styled", "css"]
			}
		]
	},
	"include": ["src/**/*"]
}
