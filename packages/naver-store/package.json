{
  "name": "@vircle/naver-store",
  "packageManager": "yarn@3.2.3",
  "version": "0.1.0",
  "description": "",
  "author": "soonj",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "release": "standard-version",
    "build": "NODE_NO_WARNINGS=1 nest build",
    "build:entity": "NODE_NO_WARNINGS=1 yarn workspace @vircle/entity build",
    "prebuild": "rimraf dist && rimraf bundle",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_NO_WARNINGS=1 NODE_ENV=local nest start",
    "start:dev": "NODE_NO_WARNINGS=1 NODE_ENV=development nest start --watch",
    "start:debug": "NODE_NO_WARNINGS=1 NODE_ENV=local nest start --debug --watch",
    "start:prod": "NODE_NO_WARNINGS=1 NODE_ENV=production node bundle/main",
    "start:stage": "NODE_NO_WARNINGS=1 NODE_ENV=stage node bundle/main",
    "start:bundle": "node bundle/server",
    "test": "NODE_NO_WARNINGS=1 NODE_ENV=local jest",
    "test:watch": "NODE_ENV=local jest --watch",
    "test:cov": "NODE_ENV=local jest --coverage",
    "test:debug": "NODE_NO_WARNINGS=1 NODE_ENV=local  node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=local jest --config ./test/jest-e2e.json",
    "lint": "NODE_NO_WARNINGS=1 eslint -c ../../.eslintrc.js --ignore-path ../../.eslintignore ./ --ext .ts,.tsx,.js,.jsx --cache",
    "bundle": "yarn bundle:prod",
    "bundle:prod": "NODE_NO_WARNINGS=1 webpack build --config ./webpack.config.js",
    "bundle:dev": "NODE_NO_WARNINGS=1 webpack build --config ./webpack.config.dev.js"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@sentry/node": "^7.38.0",
    "@sentry/tracing": "^7.38.0",
    "@vircle/entity": "workspace:*",
    "aws-sdk": "^2.1328.0",
    "axios": "^1.3.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "joi": "^17.7.1",
    "js-yaml": "^4.1.0",
    "luxon": "^3.2.1",
    "mongoose": "^7.0.0",
    "nest-winston": "^1.8.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "src": "link:./src",
    "typeorm": "^0.3.12",
    "winston": "^3.8.2",
    "winston-cloudwatch": "^6.1.1",
    "winston-sentry-log": "^1.0.26",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.3.9",
    "@types/bcryptjs": "^2",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/js-yaml": "^4.0.5",
    "@types/luxon": "^3",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@types/webpack-node-externals": "^3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "expect-type": "^0.15.0",
    "jest": "29.3.1",
    "jest-pnp-resolver": "^1.2.3",
    "pm2": "^5.2.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "standard-version": "^9.5.0",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.6.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.0.0"
  }
}
