{
	"name": "@vircle/shipping-tracking",
	"packageManager": "yarn@3.2.0",
	"main": "./src/index.ts",
	"exports": {
		".": {
			"default": "./dist/src/index.js"
		}
	},
	"description": "배송 추적 서버",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "NODE_NO_WARNINGS=1 nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "NODE_NO_WARNINGS=1 NODE_ENV=development nest start",
		"start:dev": "NODE_NO_WARNINGS=1 NODE_ENV=development nest start --watch",
		"start:debug": "NODE_NO_WARNINGS=1 NODE_ENV=development nest start --debug --watch",
		"start:prod": "NODE_ENV=production node dist/main",
		"test": "NODE_NO_WARNINGS=1 NODE_ENV=test jest",
		"test:watch": "NODE_NO_WARNINGS=1 NODE_ENV=test jest --watch",
		"test:cov": "NODE_NO_WARNINGS=1 NODE_ENV=test jest --coverage",
		"test:debug": "NODE_NO_WARNINGS=1 NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/common": "^8.0.0",
		"@nestjs/config": "^2.1.0",
		"@nestjs/core": "^8.0.0",
		"@nestjs/platform-express": "^8.0.0",
		"axios": "^0.27.2",
		"cache-manager": "^4.0.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"luxon": "^2.4.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.0.0",
		"@nestjs/schematics": "^8.0.0",
		"@nestjs/testing": "^8.0.0",
		"@types/cache-manager": "^4.0.0",
		"@types/express": "^4.17.13",
		"@types/hbs": "^4",
		"@types/jest": "27.4.1",
		"@types/luxon": "^2",
		"@types/node": "^17.0.31",
		"@types/supertest": "^2.0.11",
		"jest": "^27.2.5",
		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"ts-jest": "^27.0.3",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.7.0",
		"tsconfig-paths": "^3.10.1",
		"typescript": "^4.6.4"
	}
}
