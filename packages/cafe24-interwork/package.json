{
	"name": "@vircle/cafe24-interwork",
	"packageManager": "yarn@3.2.0",
	"version": "0.0.1",
	"description": "",
	"author": "yaeger",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist && rimraf bundle",
		"bundle": "NODE_NO_WARNINGS=1 webpack",
		"build": "NODE_NO_WARNINGS=1 nest build",
		"build:entity": "NODE_NO_WARNINGS=1 yarn workspace @vircle/entity build",
		"start": "NODE_NO_WARNINGS=1 NODE_ENV=development nest start",
		"start:dev": "NODE_NO_WARNINGS=1 NODE_ENV=development nest start --watch",
		"start:debug": "NODE_NO_WARNINGS=1 NODE_ENV=development nest start --debug --watch",
		"start:prod": "node dist/main",
		"test": "NODE_NO_WARNINGS=1 jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage"
	},
	"dependencies": {
		"@grpc/grpc-js": "^1.6.8",
		"@grpc/proto-loader": "^0.7.0",
		"@nestjs/common": "^8.0.0",
		"@nestjs/config": "^2.1.0",
		"@nestjs/core": "^8.0.0",
		"@nestjs/jwt": "^9.0.0",
		"@nestjs/mapped-types": "*",
		"@nestjs/platform-express": "^8.0.0",
		"@nestjs/platform-socket.io": "^9.0.5",
		"@nestjs/websockets": "^9.0.5",
		"@vircle/entity": "workspace:*",
		"amqp-connection-manager": "^4.1.3",
		"amqplib": "^0.10.0",
		"aws-sdk": "^2.1177.0",
		"axios": "^0.27.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"ioredis": "^5.2.2",
		"kafkajs": "^2.1.0",
		"luxon": "^3.0.1",
		"morgan": "^1.10.0",
		"mqtt": "^4.3.7",
		"nats": "^2.7.1",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.0.0",
		"@nestjs/microservices": "^9.0.5",
		"@nestjs/schematics": "^8.0.0",
		"@nestjs/testing": "^8.0.0",
		"@types/amqplib": "^0",
		"@types/express": "^4.17.13",
		"@types/jest": "27.4.1",
		"@types/luxon": "^3",
		"@types/morgan": "^1",
		"@types/node": "^17.0.31",
		"@types/supertest": "^2.0.11",
		"jest": "^27.2.5",
		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"ts-jest": "^27.0.3",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.7.0",
		"tsconfig-paths": "^3.10.1",
		"typescript": "^4.6.4",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-node-externals": "^3.0.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
