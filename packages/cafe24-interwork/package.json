{
	"name": "@vircle/cafe24-interwork",
	"packageManager": "yarn@3.2.0",
	"version": "0.0.1",
	"description": "",
	"author": "yaeger",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "NODE_NO_WARNINGS=1 nest build",
		"start": "NODE_NO_WARNINGS=1 NODE_ENV=development nest start",
		"start:dev": "NODE_NO_WARNINGS=1 NODE_ENV=development nest start --watch",
		"start:debug": "NODE_NO_WARNINGS=1 NODE_ENV=development nest start --debug --watch",
		"start:prod": "NODE_ENV=production node dist/main",
		"test": "NODE_NO_WARNINGS=1 jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage"
	},
	"dependencies": {
		"@nestjs/common": "^8.0.0",
		"@nestjs/config": "^2.1.0",
		"@nestjs/core": "^8.0.0",
		"@nestjs/jwt": "^9.0.0",
		"@nestjs/mapped-types": "*",
		"@nestjs/platform-express": "^8.0.0",
		"@vircle/entity": "workspace:*",
		"aws-sdk": "^2.1177.0",
		"axios": "^0.27.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"luxon": "^3.0.1",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.0.0",
		"@nestjs/schematics": "^8.0.0",
		"@nestjs/testing": "^8.0.0",
		"@types/express": "^4.17.13",
		"@types/jest": "27.4.1",
		"@types/luxon": "^3",
		"@types/node": "^17.0.31",
		"@types/supertest": "^2.0.11",
		"jest": "^27.2.5",
		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"ts-jest": "^27.0.3",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.7.0",
		"tsconfig-paths": "^3.10.1",
		"typescript": "^4.6.4"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
